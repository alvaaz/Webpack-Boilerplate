header#header
	.header-inner.clearfix
		#logo
			a(href='/')
				img#dark-logo(src='images/logo-dark.png', srcset='images/logo-dark.png 1x, images/logo-dark@2x.png 2x', alt='Logo Dark')
				img#light-logo(src='images/logo-light.png', srcset='images/logo-light.png 1x, images/logo-light@2x.png 2x', alt='Logo Light')
		#menu.clearfix.transition-enabled
			.menu-actions
				.menu-toggle
					span.hamburger
			#menu-inner
				+navbar(pageTitle, menu)
		#header-widget.social
			+lists('socialmedia-widget', social)